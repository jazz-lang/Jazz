import "libc.jazz"
import "atomic.jazz"

pub func alloc(size: i32) *u8 {
	return malloc(size as usize);
}

pub func alloc(size: usize) *u8 {
	return malloc(size);
}

pub func alloc(size: i64) *u8 {
	return malloc(size as usize);
}

pub func alloc(size: u32) *u8 {
	return malloc(size as usize);
}

pub func alloc(size: u64) *u8 {
	return malloc(size as usize);
}