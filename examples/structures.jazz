extern func printf(fmt: *char,...) void;
extern func calloc(c: i32,size: i32) *u8;

pub struct Point {
	x: i32,
	y: i32
}

pub func point_print(p: *Point) void {
	printf("(%i;%i)\n",p.x,p.y);
	return;
}

pub func main() i32 {
    var p: *Point;
    p = calloc(1,8) as *Point;
    p.x = 3;
    p.y = 4;
    point_print(p);
	return 0;
}